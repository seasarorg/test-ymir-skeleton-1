<actions>
  <action>
    <actionId>run-sql2entity</actionId>
    <name>SQLからEntityクラスを生成（sql2entity）</name>
    <class>org.seasar.ymir.skeleton.action.RunSql2EntityAction</class>
  </action>
  <action>
    <actionId>run-outside-sql-test</actionId>
    <name>外だしSQLの一括テスト（outside-sql-test）</name>
    <class>
      org.seasar.ymir.skeleton.action.RunOutsideSqlTestAction
    </class>
  </action>
  <action>
    <actionId>run-replace-schema</actionId>
    <name>スキーマを置き換える（replace-schema）</name>
    <class>
      org.seasar.ymir.skeleton.action.RunReplaceSchemaAction
    </class>
  </action>
  <action>
    <actionId>run-jdbc</actionId>
    <name>DBからスキーマ情報を取得（jdbc）</name>
    <class>org.seasar.ymir.skeleton.action.RunJdbcAction</class>
  </action>
  <action>
    <actionId>run-generate</actionId>
    <name>Javaクラスを生成（generate）</name>
    <class>org.seasar.ymir.skeleton.action.RunGenerateAction</class>
  </action>
  <action>
    <actionId>run-doc</actionId>
    <name>スキーマ情報をHTML形式で出力（doc）</name>
    <class>org.seasar.ymir.skeleton.action.RunDocAction</class>
  </action>
</actions>
